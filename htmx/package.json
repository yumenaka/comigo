{
  "name": "comigo",
  "version": "1.0.0",
  "description": "Frontend for Comigo.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "parcel build ./embed_files/assets/scripts.js ./embed_files/assets/styles.scss --dist-dir embed_files/static",
    "dev": "parcel build ./embed_files/assets/scripts.js ./embed_files/assets/styles.scss --dist-dir embed_files/static --no-optimize",
    "watch": "parcel watch ./embed_files/assets/scripts.js ./embed_files/assets/styles.scss --dist-dir embed_files/static --no-optimize"
  },
  "dependencies": {
    "@alpinejs/morph": "latest",
    "@alpinejs/persist": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "alpinejs": "latest",
    "flowbite": "latest",
    "htmx.org": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "screenfull": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "@tailwindcss/postcss": "^4.0.6",
    "autoprefixer": "latest",
    "parcel": "latest",
    "postcss": "latest",
    "prettier": "latest"
  }
}
