{
  "name": "comigo",
  "version": "1.0.0",
  "description": "Frontend for Comigo.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "parcel build assets/scripts.js ./assets/styles.css --dist-dir embed/static",
    "dev": "parcel build assets/scripts.js ./assets/styles.css --dist-dir embed/static --no-optimize",
    "watch": "parcel watch assets/scripts.js ./sassets/styles.css --dist-dir embed/static --no-optimize"
  },
  "dependencies": {
    "@alpinejs/morph": "latest",
    "@alpinejs/persist": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "alpinejs": "latest",
    "flowbite": "latest",
    "htmx.org": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "screenfull": "latest",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "@tailwindcss/postcss": "^4.0.6",
    "autoprefixer": "latest",
    "parcel": "latest",
    "postcss": "^8.5.1",
    "prettier": "latest"
  }
}
