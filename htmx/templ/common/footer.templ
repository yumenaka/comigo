package common

templ Footer(version string) {
	<footer class="bg-base-100 text-base-content w-full p-0 m-0 footer border-t border-slate-400">
		<aside class="flex flex-row justify-center w-full h-10 py-2 h footer item-center">
			<a class="text-sm font-semibold text-blue-700 hover:underline" href="https://comigo.xyz" target="_blank">
				Power by Comigo { version }
			</a>
		</aside>
	</footer>
}
