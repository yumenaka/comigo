package settings_page

import "github.com/yumenaka/comigo/htmx/state"

templ tab2(s *state.GlobalState) {
<div class="flex flex-col justify-start w-3/5 min-w-[24rem]">
    @NumberConfig("Port",1234,"Port_Description")
    @StringConfig("Host","","Host_Description")
    @BoolConfig("DisableLAN",true,"DisableLAN_Description")
    @BoolConfig("EnableLogin",true,"EnableLogin_Description")
    @NumberConfig("Timeout",1234,"Timeout_Description")
</div>
}