{
  "name": "comigo",
  "version": "1.0.0",
  "description": "Frontend for Comigo.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "parcel build ./assets/main.js  --dist-dir assets/script &&  bunx @tailwindcss/cli -i ./assets/styles.css -o ./assets/script/styles.css",
    "dev": "parcel build ./assets/main.js  --dist-dir assets/script --no-optimize &&  bunx @tailwindcss/cli -i ./assets/styles.css -o ./assets/script/styles.css",
    "watch": "parcel watch ./assets/main.js --dist-dir assets/script --no-optimize &&  bunx @tailwindcss/cli -i ./assets/styles.css -o ./assets/script/styles.css"
  },
  "dependencies": {
    "@alpinejs/intersect": "^3.15.2",
    "@alpinejs/morph": "latest",
    "@alpinejs/persist": "latest",
    "@tailwindcss/cli": "^4.1.17",
    "alpinejs": "latest",
    "flowbite": "latest",
    "htmx.org": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "screenfull": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "@tailwindcss/postcss": "latest",
    "@parcel/transformer-css": "latest",
    "parcel": "latest",
    "postcss": "latest",
    "prettier": "latest"
  }
}
